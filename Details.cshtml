@model STUDENT.Models.Person

<div class="container">
<h1>Person Details</h1>

<table class="table table-striped">
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Age</th>
        <td>@Model.Age</td>
    </tr>
</table>

<a href="@Url.Action("Index", "Person")" class="btn btn-secondary">Back to List</a>
</div>